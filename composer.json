{
    "name": "kodio/laravel-messaging",
    "description": "Laravel messaging package",
    "license": "MIT",
    "authors": [
        {
            "name": "Kodio Techologies",
            "email": "hola@kodio.tech",
            "homepage": "https://www.kodio.tech"
        }
    ],
    "homepage": "https://github.com/kodio/laravel-messaging",
    "keywords": ["Laravel", "LaravelMessaging"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Kodio\\LaravelMessaging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodio\\LaravelMessaging\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kodio\\LaravelMessaging\\LaravelMessagingServiceProvider"
            ],
            "aliases": {
                "LaravelMessaging": "Kodio\\LaravelMessaging\\Facades\\LaravelMessaging"
            }
        }
    }
}
